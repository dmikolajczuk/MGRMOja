<Window x:Class="MGRMikolajczuk.View.OrderDetailWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MGRMikolajczuk.View"
        mc:Ignorable="d"
        Title="OrderDetailWindow" Height="600" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>
        <DockPanel x:Name="DockPanelProcuctList" Grid.Column="0" Margin="10,20,10,10" LastChildFill="False" >
            <TextBlock Text="Lista poduktów" TextAlignment="Center" FontSize="20" DockPanel.Dock="Top"/>
            <TextBlock Text="Suma:" TextAlignment="Right" FontSize="20" DockPanel.Dock="Bottom"/>
        </DockPanel>
        <UniformGrid Grid.Column="1" x:Name="UniformGridNewProduct" Columns="5" Rows="8" Margin="10,10">
        </UniformGrid>
    </Grid>
</Window>
